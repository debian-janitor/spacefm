#, c-format
msgid "Usage:\n"
msgstr "Utilização:\n"

#: ../src/cust-dialog.c:3694
#, c-format
msgid "    spacefm --dialog|-g {ELEMENT [OPTIONS] [ARGUMENTS...]} ...\n"
msgstr "    spacefm --dialog|-g {ELEMENTO [OPÇÕES] [ARGUMENTOS...]} ...\n"

#: ../src/cust-dialog.c:3695
#, c-format
msgid "Example:\n"
msgstr "Exemplo:\n"

#: ../src/cust-dialog.c:3696
#, c-format
msgid "    spacefm -g --label \"A message\" --button ok\n"
msgstr "    spacefm -g --label \"Mensagem\" --button ok\n"

